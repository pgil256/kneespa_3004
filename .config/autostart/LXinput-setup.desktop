[Desktop Entry]
Type=Application
Name=LXInput autostart
Comment=Setup keyboard and mouse using settings done in LXInput
NoDisplay=true
Exec=sh -c 'xset m 20/10 10 r rate 500 33 b on; xinput --set-prop "pointer:Telink Wireless Receiver Mouse" "libinput Accel Speed" 1.000000; xinput --set-prop "pointer:vnc-relpointer" "libinput Accel Speed" 1.000000'
NotShowIn=GNOME;KDE;XFCE;
